{
    "version": 2,
    "builds": [{ "src": "./index.js", "use": "@vercel/node"}],
    "routes": [
      { "src": "/auth", "dest": "./src/auth.js" },
      { "src": "/controllers/(.*)", "dest": "./src/controllers/$1" },
      { "src": "/routes/(.*)", "dest": "./src/routes/$1" },
      { "src": "/(.*)", "dest": "./index.js" }
    ]
  }
  